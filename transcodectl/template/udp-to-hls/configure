#!/bin/bash

_id="$1"

echo "export UDP2HLS_AUDIO_CODEC=${UDP2HLS_AUDIO_CODEC:-copy}" > "${TRANSCODE_INSTANCED_PATH}/${_id}/build.env"
echo "export UDP2HLS_AUDIO_BITRATE=${UDP2HLS_AUDIO_BITRATE:-}" >> "${TRANSCODE_INSTANCED_PATH}/${_id}/build.env"
echo "export UDP2HLS_HLS_BASE_URL=${UDP2HLS_HLS_BASE_URL:-}" >> "${TRANSCODE_INSTANCED_PATH}/${_id}/build.env"
echo "export UDP2HLS_HLS_FLAGS=${UDP2HLS_HLS_FLAGS:-}" >> "${TRANSCODE_INSTANCED_PATH}/${_id}/build.env"
echo "export UDP2HLS_HLS_LIST_SIZE=${UDP2HLS_HLS_LIST_SIZE:-}" >> "${TRANSCODE_INSTANCED_PATH}/${_id}/build.env"
echo "export UDP2HLS_HLS_SEGMENT_FILENAME=${UDP2HLS_HLS_SEGMENT_FILENAME:-}" >> "${TRANSCODE_INSTANCED_PATH}/${_id}/build.env"
echo "export UDP2HLS_HLS_TIME=${UDP2HLS_HLS_TIME:-}" >> "${TRANSCODE_INSTANCED_PATH}/${_id}/build.env"
echo "export UDP2HLS_INPUT_CUVID=${UDP2HLS_INPUT_CUVID:-}" >> "${TRANSCODE_INSTANCED_PATH}/${_id}/build.env"
echo "export UDP2HLS_INPUT_DEINTERLACE=${UDP2HLS_INPUT_DEINTERLACE:-}" >> "${TRANSCODE_INSTANCED_PATH}/${_id}/build.env"
echo "export UDP2HLS_INPUT_RESIZE=${UDP2HLS_INPUT_RESIZE:-}" >> "${TRANSCODE_INSTANCED_PATH}/${_id}/build.env"
echo "export UDP2HLS_META_SERVICE_NAME=${UDP2HLS_META_SERVICE_NAME:-}" >> "${TRANSCODE_INSTANCED_PATH}/${_id}/build.env"
echo "export UDP2HLS_META_SERVICE_PROVIDER=${UDP2HLS_META_SERVICE_PROVIDER:-}" >> "${TRANSCODE_INSTANCED_PATH}/${_id}/build.env"
echo "export UDP2HLS_VIDEO_BITRATE=${UDP2HLS_VIDEO_BITRATE:-}" >> "${TRANSCODE_INSTANCED_PATH}/${_id}/build.env"
echo "export UDP2HLS_VIDEO_CODEC=${UDP2HLS_VIDEO_CODEC:-copy}" >> "${TRANSCODE_INSTANCED_PATH}/${_id}/build.env"
echo "export UDP2HLS_VIDEO_FRAMERATE=${UDP2HLS_VIDEO_FRAMERATE:-}" >> "${TRANSCODE_INSTANCED_PATH}/${_id}/build.env"
echo "export UDP2HLS_VIDEO_KEYFRAME=${UDP2HLS_VIDEO_KEYFRAME:-}" >> "${TRANSCODE_INSTANCED_PATH}/${_id}/build.env"
